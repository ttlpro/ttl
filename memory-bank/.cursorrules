async bulkMoveProxiesToFolder(proxyIds, folderId) {
  try {
    if (!Array.isArray(proxyIds) || proxyIds.length === 0) {
      return { success: false, error: 'Danh sÃ¡ch proxy khÃ´ng há»£p lá»‡' };
    }
    
    console.log(`ğŸ”„ Di chuyá»ƒn ${proxyIds.length} proxy vÃ o folder ${folderId}`);
    
    const now = new Date().toISOString();
    const stmt = this.db.prepare(`
      UPDATE proxies 
      SET folderId = ?, updatedAt = ? 
      WHERE id IN (${proxyIds.map(() => '?').join(',')})
    `);
    
    const params = [folderId, now, ...proxyIds];
    const result = stmt.run(...params);
    
    return { 
      success: true, 
      message: `ÄÃ£ di chuyá»ƒn ${result.changes} proxy sang folder má»›i`,
      count: result.changes 
    };
