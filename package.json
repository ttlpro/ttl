{
  "private": true,
  "name": "amac-tiktok-viewer",
  "description": "TTL - Tool TikTok Live Viewer Booster",
  "version": "1.0.2",
  "author": "TTL Tools",
  "main": "app/background.js",
  "homepage": "./",
  "scripts": {
    "dev": "nextron",
    "build": "nextron build",
    "build:mac": "nextron build --mac",
    "build:mac:nosign": "CSC_IDENTITY_AUTO_DISCOVERY=false nextron build --mac",
    "build:mac:universal": "nextron build --mac --universal",
    "build:linux": "nextron build --linux",
    "build:win32": "nextron build --win --ia32",
    "build:win64": "nextron build --win --x64",
    "postinstall": "electron-builder install-app-deps",
    "check-translations": "node scripts/check-translations.js",
    "i18n:check": "npm run check-translations",
    "check-i18n": "node scripts/check-i18n.js",
    "test:migration": "node test-migration.js",
    "test:migration:perf": "node test-migration.js --perf",
    "test:verify": "node scripts/verify-migration.js",
    "migration:force": "node scripts/migration-tools.js force",
    "migration:rollback": "node scripts/migration-tools.js rollback",
    "db:info": "node scripts/db-info.js",
    "db:check": "electron scripts/test-migration-production.js",
    "electron:build:win": "electron-builder --win --publish=never",
    "electron:build:mac": "electron-builder --mac --publish=never",
    "electron:build:linux": "electron-builder --linux --publish=never",
    "electron:build:all": "electron-builder --win --mac --linux --publish=never",
    "electron:publish": "electron-builder --publish=always",
    "version:patch": "npm version patch && git push && git push --tags",
    "version:minor": "npm version minor && git push && git push --tags",
    "version:major": "npm version major && git push && git push --tags",
    "release:patch": "npm run version:patch",
    "release:minor": "npm run version:minor",
    "release:major": "npm run version:major",
    "release:test": "node scripts/test-release.js",
    "release:auto": "node scripts/release-automation.js",
    "release:auto:test": "node scripts/release-automation.js --test",
    "test:git-auth": "node scripts/test-git-auth.js",
    "test:push-access": "node scripts/test-push-access.js",
    "test:clean-push": "node scripts/test-clean-push.js",
    "test:verify-push": "node scripts/test-verify-push.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.16",
    "axios": "^1.9.0",
    "better-sqlite3": "^11.10.0",
    "electron-serve": "^1.3.0",
    "electron-store": "^10.0.1",
    "https-proxy-agent": "^7.0.6",
    "i18next": "^25.2.1",
    "lucide-react": "^0.511.0",
    "next-i18next": "^15.4.2",
    "postcss": "^8.4.35",
    "proxy-agent": "^6.5.0",
    "puppeteer": "^24.9.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.5.2",
    "react-toastify": "^11.0.5",
    "request": "^2.88.2",
    "socks-proxy-agent": "^8.0.5",
    "tailwindcss": "^3.4.10",
    "tunnel": "^0.0.6"
  },
  "devDependencies": {
    "@octokit/rest": "^22.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9",
    "next": "^14.2.4",
    "nextron": "^9.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
